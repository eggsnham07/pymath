#!/bin/bash
if [[ "$1" == add* ]]; then
   /usr/bin/env python3 ~/.local/bin/pymath/pylib/add.py $@
elif [[ "$1" == sub* ]]; then
   /usr/bin/env python3 ~/.local/bin/pymath/pylib/sub.py $@
elif [[ "$1" == div* ]]; then
   /usr/bin/env python3 ~/.local/bin/pymath/pylib/div.py $@
elif [[ "$1" == mul* ]]; then
   /usr/bin/env python3 ~/.local/bin/pymath/pylib/mult.py $@
elif [[ "$1" == sqr* ]]; then
   /usr/bin/env python3 ~/.local/bin/pymath/pylib/sqrt.py $@
elif [[ "$1" == gui* ]]; then
   /usr/bin/env python3 ~/.local/bin/pymath/app.py gui
else
   echo "Invalid operation"
fi
